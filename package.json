{
  "name": "liteend",
  "version": "0.0.1",
  "description": "Lightweight, fast, and easy to use backend app template for Node.js",
  "author": "uxname",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && npm run check",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "update": "npx ncu -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
    "db:push": "npx prisma db push && npm run gen",
    "db:migrations:apply": "npx prisma migrate deploy",
    "postdb:migrations:apply": "npm run db:gen",
    "db:migrations:create": "npx prisma migrate dev --create-only",
    "db:schema:format": "npx prisma format",
    "db:gen": "npx prisma generate",
    "db:studio": "npx prisma studio --browser none",
    "db:studio-local": "npm run db:studio -- -n 127.0.0.1",
    "db:seed": "ts-node src/seed.ts",
    "start": "nest start",
    "start:dev": "npx cross-env NODE_ENV=development nest start --watch",
    "start:debug": "npx cross-env NODE_ENV=development nest start --debug --watch",
    "prestart:prod": "npm run db:migrations:apply",
    "start:prod": "npx cross-env NODE_ENV=production node --enable-source-maps dist/src/main",
    "ts:check": "tsc --noEmit",
    "check": "npm run ts:check && npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/gateway": "^2.3.2",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/platform-express": "^9.3.9",
    "@prisma/client": "^4.11.0",
    "apollo-server-express": "^3.12.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express-basic-auth": "^1.2.1",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^6.0.1",
    "log4js": "^6.9.0",
    "log4js-json-layout": "^2.2.3",
    "nodemailer": "^6.9.1",
    "prisma-nestjs-graphql": "^17.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "serve-index": "^1.9.1",
    "ts-morph": "^17.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^5.4.1",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-local": "^1.0.35",
    "@types/serve-index": "^1.9.1",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "29.5.0",
    "npm-check-updates": "^16.7.10",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "rimraf": "^4.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.5"
  }
}
