{
  "name": "liteend",
  "version": "0.0.1",
  "description": "Lightweight, fast, and easy to use backend app template for Node.js",
  "author": "uxname",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && npm run check",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "update": "npx ncu -u",
    "db:push": "npx prisma db push && npm run gen",
    "db:migrations:apply": "npx prisma migrate deploy",
    "postdb:migrations:apply": "npm run db:gen",
    "db:migrations:create": "npx prisma migrate dev --create-only",
    "db:schema:format": "npx prisma format",
    "db:gen": "npx prisma generate",
    "db:studio": "npx prisma studio --browser none",
    "db:studio-local": "npm run db:studio -- -n 127.0.0.1",
    "db:seed": "ts-node src/seed.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "ts:check": "tsc --noEmit",
    "check": "npm run ts:check && npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "nestjs-easyconfig": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^5.4.1",
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.15",
    "@types/jest": "29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "jest": "29.3.1",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.4"
  }
}
