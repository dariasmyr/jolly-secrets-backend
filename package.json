{
    "name": "liteend",
    "version": "1.0.0",
    "main": "src/index.ts",
    "license": "MIT",
    "type": "commonjs",
    "engines": {
        "node": ">=13.0.0"
    },
    "scripts": {
        "start": "NODE_ENV=production ts-node src/index.ts",
        "dev": "ts-node-dev --respawn --transpile-only --clear --exit-child --rs src/index.ts",
        "prisma:push": "npx prisma db push && npm run gen",
        "prisma:deploy": "npx prisma migrate deploy",
        "prisma:create": "npx prisma migrate dev",
        "prisma:gen": "npx prisma generate",
        "prisma:studio": "npx prisma studio --browser none",
        "prisma:format": "npx prisma format",
        "postprisma:deploy": "npm run prisma:gen",
        "gen": "npm run prisma:gen && npm run ts:gen",
        "ts:gen": "graphql-codegen --config codegen.yml",
        "ts:check": "tsc --noEmit",
        "lint": "./node_modules/.bin/eslint . --fix",
        "check": "npm run ts:check && npm run lint",
        "update": "npx ncu -u",
        "test": "jest"
    },
    "dependencies": {
        "@prisma/client": "^3.4.1",
        "@types/ua-parser-js": "^0.7.36",
        "apollo-server-express": "^3.5.0",
        "apollo-tracing": "^0.15.0",
        "bcrypt": "^5.0.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-basic-auth": "^1.2.0",
        "express-rate-limit": "^5.5.1",
        "geoip-lite": "^1.4.2",
        "graphql": "^16.0.1",
        "graphql-cost-analysis": "^1.0.3",
        "helmet": "^4.6.0",
        "lodash": "^4.17.21",
        "log4js": "^6.3.0",
        "multer": "^1.4.3",
        "nanoid": "^3.1.30",
        "nodemailer": "^6.7.0",
        "serve-index": "^1.9.1",
        "ua-parser-js": "^1.0.2",
        "validate.js": "^0.13.1"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^3.1.0",
        "@graphql-codegen/cli": "2.2.2",
        "@graphql-codegen/introspection": "2.1.0",
        "@graphql-codegen/typescript": "2.3.1",
        "@graphql-codegen/typescript-resolvers": "2.4.0",
        "@jetbrains/eslint-config": "^5.3.1",
        "@types/bcrypt": "^5.0.0",
        "@types/express-rate-limit": "^5.1.3",
        "@types/geoip-lite": "^1.4.1",
        "@types/jest": "^27.0.2",
        "@types/lodash": "^4.14.176",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.11.6",
        "@types/nodemailer": "^6.4.4",
        "@types/serve-index": "^1.9.1",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "eslint": "^8.2.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-sonarjs": "^0.10.0",
        "jest": "^27.3.1",
        "npm-check-updates": "^12.0.0",
        "prisma": "^3.4.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    }
}
