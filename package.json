{
    "name": "liteend",
    "version": "1.0.0",
    "main": "src/index.ts",
    "license": "MIT",
    "type": "commonjs",
    "engines": {
        "node": ">=13.0.0"
    },
    "scripts": {
        "prestart": "npx eslint .",
        "start": "NODE_ENV=production ts-node src/app.ts",
        "dev": "ts-node-dev --respawn --transpile-only --clear --exit-child --rs src/app.ts",
        "db:push": "npx prisma db push && npm run gen",
        "db:deploy": "npx prisma migrate deploy",
        "db:create": "npx prisma migrate dev",
        "db:gen": "npx prisma generate",
        "db:studio": "npx prisma studio --browser none",
        "db:format": "npx prisma format",
        "postdb:deploy": "npm run db:gen",
        "gen": "npm run db:gen && npm run ts:gen",
        "ts:gen": "graphql-codegen --config codegen.yml",
        "ts:check": "tsc --noEmit",
        "lint": "npx eslint . --fix",
        "check": "npm run ts:check && npm run lint",
        "update": "npx ncu -u",
        "test": "jest"
    },
    "dependencies": {
        "@prisma/client": "^4.1.0",
        "@types/ua-parser-js": "^0.7.36",
        "apollo-server-express": "^3.10.0",
        "apollo-tracing": "^0.15.0",
        "bcrypt": "^5.0.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "express-basic-auth": "^1.2.1",
        "express-rate-limit": "^6.5.1",
        "geoip-lite": "^1.4.5",
        "graphql": "^16.5.0",
        "graphql-cost-analysis": "^1.0.3",
        "helmet": "^5.1.1",
        "lodash": "^4.17.21",
        "log4js": "^6.6.0",
        "multer": "^1.4.4",
        "nanoid": "^3.3.4",
        "nodemailer": "^6.7.7",
        "serve-index": "^1.9.1",
        "ua-parser-js": "^1.0.2",
        "validate.js": "^0.13.1",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^3.2.0",
        "@graphql-codegen/cli": "^2.9.1",
        "@graphql-codegen/introspection": "2.2.0",
        "@graphql-codegen/typescript": "2.7.2",
        "@graphql-codegen/typescript-graphql-request": "^4.5.2",
        "@graphql-codegen/typescript-operations": "^2.5.2",
        "@graphql-codegen/typescript-resolvers": "2.7.2",
        "@jetbrains/eslint-config": "^5.4.1",
        "@types/bcrypt": "^5.0.0",
        "@types/express-rate-limit": "^5.1.3",
        "@types/geoip-lite": "^1.4.1",
        "@types/jest": "^28.1.6",
        "@types/lodash": "^4.14.182",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.6.1",
        "@types/nodemailer": "^6.4.4",
        "@types/serve-index": "^1.9.1",
        "@types/validator": "^13.7.4",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "eslint": "^8.20.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-security": "^1.5.0",
        "eslint-plugin-sonarjs": "^0.14.0",
        "graphql-request": "^4.3.0",
        "jest": "^28.1.3",
        "npm-check-updates": "^16.0.0",
        "prisma": "^4.1.0",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    }
}
