{
    "name": "backend",
    "version": "0.0.1",
    "description": "Lightweight, fast, and easy to use backend app template for Node.js",
    "author": "uxname",
    "private": true,
    "license": "MIT",
    "scripts": {
        "________________ BUILD AND RUN ________________": "",
        "save-commit-info": "npx ts-node src/common/git-commit-saver.ts",
        "prebuild": "rimraf dist && npm run lint && npm run save-commit-info",
        "build": "nest build",
        "start:dev": "npx cross-env NODE_ENV=development nest start --watch",
        "start:debug": "npx cross-env NODE_ENV=development nest start --debug --watch",
        "prestart:prod": "npm run db:migrations:apply",
        "start:prod": "npx cross-env NODE_ENV=production node --enable-source-maps dist/src/main",
        "________________ FORMAT AND LINT ________________": "",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "check": "npx run-p ts:check lint",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "ts:check": "tsc --noEmit",
        "________________ TEST ________________": "",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "________________ DATABASE ________________": "",
        "db:push": "npx prisma db push && npm run db:gen",
        "db:reset": "npx prisma migrate reset --force && npm run db:push && npm run db:gen",
        "db:migrations:apply": "npx prisma migrate deploy && npm run db:gen",
        "db:migrations:create": "npx prisma migrate dev --create-only",
        "db:schema:format": "npx prisma format",
        "db:gen": "npx rimraf src/@generated/nestgraphql && npx prisma generate",
        "db:studio": "npx prisma studio --browser none",
        "db:studio-local": "npm run db:studio -- -n 127.0.0.1",
        "db:seed": "ts-node src/seed.ts",
        "________________ OTHER ________________": "",
        "postinstall": "npm run prepare",
        "update": "npx npm-check-updates -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
        "postupdate": "npm run check && npm run lint:fix",
        "prepare": "husky"
    },
    "dependencies": {
        "@apollo/gateway": "^2.9.0",
        "@apollo/server": "^4.11.0",
        "@dicebear/collection": "^9.2.1",
        "@dicebear/core": "^9.2.1",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/apollo": "^12.2.0",
        "@nestjs/bull": "^10.2.1",
        "@nestjs/common": "^10.4.1",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.4.1",
        "@nestjs/graphql": "^12.2.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/platform-express": "^10.4.1",
        "@nestjs/platform-socket.io": "^10.4.1",
        "@nestjs/swagger": "^7.4.0",
        "@nestjs/websockets": "^10.4.1",
        "@prisma/client": "^5.19.1",
        "apollo-server-express": "^3.13.0",
        "axios": "^1.7.7",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "bull": "^4.16.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "google-auth-library": "^9.14.1",
        "graphql": "^16.9.0",
        "graphql-type-json": "^0.3.2",
        "helmet": "^7.1.0",
        "http-proxy-middleware": "^3.0.2",
        "i18next": "^23.15.1",
        "log4js": "^6.9.1",
        "log4js-json-layout": "^2.2.3",
        "nestjs-i18n": "^10.4.9",
        "nodemailer": "^6.9.15",
        "prisma-nestjs-graphql": "^20.0.3",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "telegraf": "^4.16.3",
        "ts-morph": "^23.0.0",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.0.1",
        "@jetbrains/eslint-config": "^5.4.2",
        "@nestjs/cli": "^10.4.5",
        "@nestjs/schematics": "^10.1.4",
        "@nestjs/testing": "^10.4.1",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/express": "^4.17.21",
        "@types/jest": "29.5.13",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.5.5",
        "@types/nodemailer": "^6.4.15",
        "@types/passport-local": "^1.0.38",
        "@types/serve-index": "^1.9.4",
        "@types/source-map-support": "^0.5.10",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.12",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-comments": "^1.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-jest": "^28.8.3",
        "eslint-plugin-markdown": "^5.1.0",
        "eslint-plugin-no-secrets": "^1.0.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-sonarjs": "^0.25.1",
        "eslint-plugin-unicorn": "^55.0.0",
        "husky": "^9.1.6",
        "jest": "29.7.0",
        "npm-check-updates": "^17.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.3.3",
        "prisma": "^5.19.1",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.6.2"
    }
}
